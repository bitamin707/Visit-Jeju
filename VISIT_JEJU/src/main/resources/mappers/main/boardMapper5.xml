<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.human.dao.main.IBoardDaoAccount">
<insert id="createAccount">
insert into account values(#{accountID},#{accountpw},#{birth},#{phoneNum},#{sex},#{email},'0')
</insert>
<select id="readAccount" resultType="BoardDtoAccount">
select * from account where accountID=#{accountID}
</select>
<update id="updateAccount">
update account set accountpw=#{accountpw},birth=#{birth},phoneNum=#{phoneNum},sex=#{sex},email=#{email},adminCheck=#{adminCheck} where accountID=#{accountID} 
</update>
<select id="listAllAccount" resultType="com.human.dto.main.BoardDtoAccount">
select * from account
</select>
</mapper>









