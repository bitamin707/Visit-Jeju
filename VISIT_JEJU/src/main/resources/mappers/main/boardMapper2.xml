<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.human.dao.main.IBoardDaoFoodBest">
<update id="updateFoodBest">
update foodbest set imgLink=#{imgLink},name=#{name},content=#{content},tag=#{tag}
</update>
<select id="listAllFoodBest" resultType="com.human.dto.main.BoardDtoFoodBest">
select * from foodbest
</select>
</mapper>









